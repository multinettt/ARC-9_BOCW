"UnlitGeneric"
{
               "$basetexture" "models\weapons\pack\pap_rainbow"

	$detail "models\weapons\pack\pap_rainbow"
	$detailscale "1"
	$detailblendmode "0"
	$detailblendfactor "0.9"

	$reflectivity "[2 2 2]"

                     "Proxies"
             		  {
   	   	"AnimatedTexture"
    		  {
     		    "animatedtexturevar" "$basetexture"
      		    "animatedtextureframenumvar" "$frame"
         		    "animatedtextureframerate" "30"
     		  }
  	 }
	
}